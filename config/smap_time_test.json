    "beta_schedule": {
      "train": {
        "schedule": "linear",
        "n_timestep": 100,
        "linear_start": 1e-6,
        "linear_end": 1e-2
      },
      "test": {
        "schedule": "linear",
        "start_label": 1,
        "end_label": 3001,
        "step_label": 1,
        "step_t": 1000,
        "n_timestep": 100,
        "linear_start": 1e-6,
        "linear_end": 1e-2
      }
    },
    "diffusion": {
      "time_size": 2048,
      "channels": 1,
      "conditional": true
    }
  },
  "wandb": {
    "project": "distributed_time"
  }
