{
  "name": "SMAP_TEST",
  "phase": "test",
  "gpu_ids": [
    0
  ],
  "path": {
    "log": "logs",
    "tb_logger": "tb_logger",
    "results": "results",
    "checkpoint": "checkpoint",
    "resume_state": "experiments/SMAP_TRAIN_128_2048_100/checkpoint/E100"
  },
  "datasets": {
    "test": {
      "name": "smap_test",
      "mode": "HR",
