  },
  "model": {
    "which_model_G": "sr3",
    "finetune_norm": false,
    "unet": {
      "in_channel": 2,
      "out_channel": 1,
      "inner_channel": 32,
      "norm_groups": 16,
      "channel_multiplier": [
        1,
